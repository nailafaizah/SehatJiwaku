        </activity>
        <activity android:name=".ui.Opening1" android:label="@string/app_name">
        </activity>
        <activity android:name=".ui.Opening2" android:label="@string/app_name">
        </activity>
        <activity android:name=".ui.Opening3" android:label="@string/app_name">
        </activity>
        <activity android:name=".ui.Login" android:label="@string/app_name">
